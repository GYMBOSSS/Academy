<Window x:Class="StudBD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudBD"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="studentRegButton" Content = "Зарегистрировать студента" Width="165" Height="40" Margin="625,55,10,339" Click="studentRegButton_Click"></Button>
        <Button x:Name="teacherRegButton" Content = "Зарегистрировать преподавателя" Width="200" Height="40" Margin="590,10,10,384" Click="teacherRegButton_Click"></Button>
        <Button x:Name="GroupCreateButton" Content = "Создать группу" Width="200" Height="40" Margin="590,128,10,266" Click="GroupCreateButton_Click"></Button>
        <TreeView Margin="0,0,567,0">
            <TextBox> Элемент TreeView</TextBox>
            <TreeViewItem Header="Community">
                <TreeViewItem Header="Students"></TreeViewItem>
                <TreeViewItem Header="Teachers"></TreeViewItem>
            </TreeViewItem>
            <TreeViewItem Header="Groups"></TreeViewItem>
        </TreeView>
    </Grid>
</Window>
